<?xml version="1.0" encoding="UTF-8"?>
<!--实时网签-自营贷款-->
<transaction className="com.pactera.fems.message.jrb.domain.business.request.RealTimeOnlineContract" fieldName="gettx"
             nodeName="gettx" type="Single" extends="JRBReqMsg" extendsfield="transaction.body" isHide="N">
    <!--自营贷款-->
    <field fieldName="customerName" nodeName="CUSTOMER_NAME" text="借款人名称" type = "String" required = "Y" length="[1,100]"/>
    <field fieldName="intRate" nodeName="INT_RATE" text="月利率" type = "Double" required = "Y" length="[1,14]" format="#.########"/>
    <field fieldName="certificateNo" nodeName="CERTIFICATE_NO" text="借款人证件号码" type = "String" required = "Y" length="[1,50]"/>
    <field fieldName="contractNo" nodeName="CONTRACT_NO" text="合同编号" type = "String" required = "Y" length="[1,100]"/>
    <field fieldName="customerType" nodeName="CUSTOMER_TYPE" text="借款人类别" type = "String" required = "Y" length="[6]"/>
    <field fieldName="loanCate" nodeName="LOAN_CATE" text="贷款类型" type = "String" required = "Y" length="[6]"/>
    <field fieldName="certificateType" nodeName="CERTIFICATE_TYPE" text="借款人证件类型" type = "String" required = "Y" length="[6]"/>
    <field fieldName="contractAmount" nodeName="CONTRACT_AMOUNT" text="合同金额" type = "Double" required = "Y" length="[1,16]" format="#.##"/>
    <field fieldName="contractSignDate" nodeName="CONTRACT_SIGN_DATE" text="合同签订日期" type = "Date" required = "Y"  format="yyyyMMdd"/>
</transaction>
